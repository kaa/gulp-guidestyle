{
  "name": "gulp-guidestyle",
  "version": "1.1.0",
  "description": "A gulp plugin to analyze stylesheets using `guidestyle`",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p src/ || true",
    "pretest": "tsc -p src/ || true",
    "test": "mocha dist/tests/ --require source-map-support/register || true",
    "clean": "rimraf dist"
  },
  "keywords": [
    "gulp",
    "guidestyle"
  ],
  "author": "kaa",
  "license": "ISC",
  "dependencies": {
    "guidestyle": "^1.2.0",
    "gulp-util": "^3.0.7",
    "through2": "^2.0.1"
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "mocha": "^2.5.3",
    "rimraf": "^2.5.4",
    "source-map-support": "^0.4.2",
    "typescript": "^2.0.10"
  }
}
